[marshalling]
serialize_format=json
deserialize_format=json

[cloudkeep]
base_url=<base_url>
api_version=v1
tenant_id=test_tenant

[cloudkeep-secrets]
name=aes_key
algorithm=aes
bit_length=256
cypher_type=CDC
plain_text=fake_data
mime_type=text/plain